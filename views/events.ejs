<% include ./partials/eheader %>
    <div class = "container">
        <div class="row">
            <div class="col-lg-12">
                <div id="event-title">
                    <h1>Upcoming Events</h1>
                    <hr>
                    <% events.forEach(function(event) { %>
                        <h2 id="name"><%=event.eventName%></h2>
                        <span id="date"><%=event.date%></span>
                        <span id="end"><%=event.endDate%></span>
                        <p id="location"><%=event.loc%></p>
                        <% for(var i= 0; i < event.day.length; i++) { %>
                               <p><%=event.day[i]%></p>
                        <% } %>
                        <% for(var i= 0; i < event.dayDescription.length; i++) { %>
                               <p id="description"><%=event.dayDescription[i]%></p>
                        <% } %>
                    <% }) %>
                </div>
            </div>
        </div>
    </div>
<% include ./partials/footer %>